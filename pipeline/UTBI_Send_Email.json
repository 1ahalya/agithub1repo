{
	"name": "UTBI_Send_Email",
	"properties": {
		"activities": [
			{
				"name": "API_Email",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"method": "POST",
					"headers": {
						"Content-Type": "application/json"
					},
					"url": {
						"value": "@pipeline().globalParameters.Email_API",
						"type": "Expression"
					},
					"body": {
						"value": "{\n    \"message\" : \"@{pipeline().parameters.message}\",\n    \"subject\" : \"@{pipeline().parameters.subject}\", \n    \"receiver\" : \"@{pipeline().parameters.receiver}\"\n   }",
						"type": "Expression"
					}
				}
			}
		],
		"parameters": {
			"receiver": {
				"type": "string",
				"defaultValue": "etl@utoronto.ca"
			},
			"subject": {
				"type": "string",
				"defaultValue": "test_pipeline"
			},
			"message": {
				"type": "string",
				"defaultValue": "Testing"
			}
		},
		"folder": {
			"name": "Control Tables"
		},
		"annotations": [],
		"lastPublishTime": "2021-12-01T22:48:10Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}